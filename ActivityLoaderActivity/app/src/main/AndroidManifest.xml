<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="intentslab.labs.course.com.activityloaderactivity" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <uses-sdk
            android:minSdkVersion="10"
            android:targetSdkVersion="21" />
        <activity
            android:name=".ActivityLoaderActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW"/>
               <!-- <action android:name="intentslab.labs.course.browseractivity.MyBrowserActivity"/>-->
                <data android:scheme="http" />
                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExplicitlyLoadedActivity"
            android:label="@string/title_activity_explicitly_loaded" >
        </activity>
    </application>

</manifest>
